          widget: "string",
          required: false,
        },
        {
          name: "width",
          label: "Width",
          widget: "number",
          value_type: "int",
          required: false,
          hint: "In px; Leave empty for auto; Useful for image optimization when not full width.",
        },
        {
          name: "aspectRatio",
          label: "Aspect Ratio",
          widget: "number",
          value_type: "float",
          hint: "Width / Height => square = 1; 16:9 = 1.78; 4:3 = 1.33; Extra wide = 4;",
