      function multilineToInline(content) {
        return content
        return typeof content === 'string' ? content.replace(/\n/g, "{% n %}") : content;
      }
      function inlineToMultiline(content) {
        return typeof content === 'string' ? content.replace(/\{% n %\}/g, "\n") : content;
      }
